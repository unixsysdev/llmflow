# LLMFlow Environment Configuration
# Copy this file to .env and set your API keys

# OpenAI API Configuration
OPENAI_API_KEY=your_openai_api_key_here

# LLMFlow LLM Configuration
LLMFLOW_LLM_PROVIDER=openai
LLMFLOW_LLM_MODEL=gpt-4
LLMFLOW_LLM_FALLBACK_MODEL=gpt-3.5-turbo
LLMFLOW_LLM_MAX_TOKENS=4000
LLMFLOW_LLM_TEMPERATURE=0.1
LLMFLOW_LLM_TIMEOUT_SECONDS=30
LLMFLOW_LLM_RETRY_ATTEMPTS=3

# LLM Optimization Configuration
LLMFLOW_LLM_OPTIMIZATION_ENABLED=true
LLMFLOW_LLM_CONFIDENCE_THRESHOLD=0.7
LLMFLOW_LLM_AUTO_APPLY_THRESHOLD=0.9
LLMFLOW_LLM_VALIDATION_REQUIRED=true

# Master Queue Configuration
LLMFLOW_MASTER_OPTIMIZATION_INTERVAL=300
LLMFLOW_MASTER_CONSENSUS_TIMEOUT=10

# Logging
LLMFLOW_LOG_LEVEL=INFO
